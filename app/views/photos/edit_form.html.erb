<h1>Edit Photo</h1>

<form action="http://localhost:3000/edit_photo<%= photo.id %>">
  <label>Caption:</label>
  <input type="text" name="the_caption" value="<%= @photo.caption %>">

  <label>Image URL:</label>
  <input type="text" name="the_url" value="<%= @photo.source %>">

  <button>Update Photo</button>

</form>
